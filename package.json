{
  "name": "vue-futurelink",
  "description": "Preload links about to be clicked with futurelink.",
  "version": "1.1.0",
  "author": "Callum Macrae <callum@samknows.com>",
  "license": "MIT",
  "main": "dist/futurelink.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SamKnows/vue-futurelink.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prepublish": "npm run build",
    "test": "jest --coverage"
  },
  "dependencies": {
    "futurelink": "^1.0.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "jest": "^24.9.0",
    "terser-webpack-plugin": "^2.0.1",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
